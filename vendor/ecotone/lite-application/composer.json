{
  "name": "ecotone/lite-application",
  "minimum-stability": "dev",
  "homepage": "https://docs.ecotone.tech/",
  "prefer-stable": true,
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Dariusz Gafka",
      "email": "dgafka.mail@gmail.com"
    }
  ],
  "keywords": [
    "ddd",
    "cqrs",
    "messaging",
    "eip",
    "distributed architecture",
    "ecotone",
    "ddd and cqrs on top of eip"
  ],
  "description": "Quickstart Ecotone Lite Application",
  "autoload": {
    "psr-4": {
      "Ecotone\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Ecotone\\": "tests"
    }
  },
  "require": {
    "ecotone/ecotone": "^1.0",
    "ecotone/jms-converter": "^1.0",
    "php-di/php-di": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0",
    "behat/behat": "^3.3.1",
    "guzzlehttp/psr7": "^1.4",
    "php-coveralls/php-coveralls": "^2.1.0",
    "phpstan/phpstan": "^0.12"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.8.4-dev"
    }
  }
}
